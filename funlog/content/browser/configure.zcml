<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  xmlns:i18n="http://namespaces.zope.org/i18n"
  i18n_domain="funlog.content">

  <browser:page
    for="*"
    name="persional_info"
    permission="funlog.isMember"
    class=".persionalInfo.PersionalInfo" />

  <browser:page
    for="*"
    name="set_persional_info"
    permission="funlog.isMember"
    class=".persionalInfo.SetPersionalInfo" />

</configure>
